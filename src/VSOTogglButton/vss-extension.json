{
  "namespace": "VSO-Toggl-Button",
  "version": "0.0.2.1",
  "name": "Toggl-Button",
  "description": "VSO Extension to integrate with toggl.com app",
  "provider": { "name": "Gerson Dias" },
  "baseUri": "https://togglbtn-vso.azurewebsites.net",
  "categories": [ "Productivity", "Sync and integration" ],
  "icons": {
    "default": "/images/apple-touch-icon-72x72.png"
  },
  "contributions": [
    {
      "id": "TogglButton",
      "type": "ms.vss-web.action",
      "description": "Track time for this workitem",
      "targets": [
        "ms.vss-work-web.work-item-toolbar-menu"
      ],
      "properties": {
        "text": "",
        "title": "TogglButton",
        "toolbarText": "Start Time",
        "icon": "images/inactive-16.png",
        "group": "actions",
        "uri": "TogglButton.html",
        "registeredObjectId": "TogglButton"
      }
    },
    {
      "id": "TogglButtonForm",
      "targets": [ ".controls" ],
      "type": "ms.vss-web.control",
      "properties": {
        "url":  "TogglButtonForm.html"
      }
    }
  ],
  "publisher": "GersonDias"
}
