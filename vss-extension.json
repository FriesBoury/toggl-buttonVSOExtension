{
  "manifestVersion": 1,
  "version": "1.1",
  "name": "Toggl-Button",
  "id": "almrangers-vsoextensions-Toggl-Button",
  
  "description": "Extension for integration between VSO and toggl.com webapp",
  "provider": { "name": "Gerson Dias" },
  "baseUri": "https://vso-toggl.azurewebsites.net",
  "categories": [ "Plan and track" ],
  "icons": {
    "default": "/public/images/apple-touch-icon-72x72.png",
    "wide": "/public/images/toggl_wide.png"
  },
  "tags": [
  	 "working",
  	 "track",
  	 "time management"
  ],
  "screenshots": [
  		{"path": "screenshots/screen1.png"},
  		{"path": "screenshots/screen2.png"}
  		{"path": "screenshots/screen3.png"}
  		{"path": "screenshots/screen4.png"}
  ],
  "content": {
     "license": {
     		"path": "license.txt"
     }
  },
  "links": {
  		"home": "aka.ms/vsoext12",
  		"repository": "https://github.com/GersonDias/toggl-buttonVSOExtension",
  		"issues": "https://github.com/GersonDias/toggl-buttonVSOExtension/issues"
  },
  "brand": {
		"color": "red",
		"theme": "dark"
  }
  "publisher": "GersonDias",
  "scopes": [
  	  "vso.profile",
  	  "vso.work",
  	  "vso.work_write"
  	  ],
  "contributions": [
    {
      "id": "TogglButton",
      "type": "ms.vss-web.action",
      "description": "Track time for this workitem",
      "targets": [
        "ms.vss-work-web.work-item-toolbar-menu"
      ],
      "properties": {
        "text": "Track your time",
        "title": "TogglButton",
        "toolbarText": "Start Time",
        "icon": "images/active-16.png",
        "group": "actions",
        "uri": "TogglButton",
        "registeredObjectId": "TogglButton"
      }
    },
    {
      "id": "TogglButtonForm",
      "targets": [ ".controls" ],
      "type": "ms.vss-web.control",
      "properties": {
        "uri":  "TogglButtonForm"
      }
    }
  ],

}
